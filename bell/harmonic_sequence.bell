## rank: 3.2
$prog = [b4 d5 f5 g5] [c5 e5 g5];
$onset = 0;
$unit = 500;
for $mod in ((0...7) * 700) % 1200 do (
    for $chord in $prog + $mod with @unwrap 1 do (
        for $mc in $chord do (
            ezsampler($mc @duration $unit).transcribe($onset) 
        );
        $onset += $unit
    ) 
);
compile(@clefs 'g')